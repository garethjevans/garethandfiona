    <!-- Custom scripts for this template -->
    <script src="/static/js/new-age.min.js"></script>
    <script>
      var d = new Date(new Date().setTime(1535374800000));
  
      simplyCountdown('.simply-countdown-one', {
          year: d.getFullYear(),
          month: d.getMonth() + 1,
          day: d.getDate(),
          hours: d.getHours(),
          minutes: d.getMinutes(),
          seconds: d.getSeconds(),
          enableUtc: false
      });
    </script>
